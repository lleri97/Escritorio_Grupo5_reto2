    <children>
        <Button fx:id="btnNewDepartment" layoutX="37.0" layoutY="25.0" mnemonicParsing="false" text="Nuevo departamento" />
        <TableView fx:id="tableDepartments" layoutX="37.0" layoutY="109.0" prefHeight="324.0" prefWidth="924.0">
            <columns>
                <TableColumn fx:id="columnID" prefWidth="152.0" text="ID" />
                <TableColumn fx:id="columnName" prefWidth="771.0" text="Nombre" />

            </columns>
        </TableView>
        <Button fx:id="btnSearchDepartment" layoutX="37.0" layoutY="65.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="84.0" text="Buscar:" />
